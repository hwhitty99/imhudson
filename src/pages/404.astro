---
import Layout from "../layouts/Layout.astro";
import obiWan from "../assets/obi-wave.gif";
---

<Layout>
  <div class="flex flex-col items-center justify-center h-screen space-y-4">
    <h1 class="text-2xl font-black">Error 404</h1>
    <p>This is not the page you're looking for.</p>
    <img
      src={obiWan}
      alt="Obi Wan Kenobi waving at you"
      class="w-1/2 pt-2 max-w-xs"
    />
  </div>
</Layout>
