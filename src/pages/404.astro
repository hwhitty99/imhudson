---
import Layout from '../layouts/Layout.astro';
import obiWan from '../assets/obi-wave.gif';
---

<Layout>
    <div
        class="my-auto flex h-full flex-col items-center justify-center space-y-4"
    >
        <h1 class="text-2xl font-black">Error 404</h1>
        <p>This is not the page you're looking for.</p>
        <img
            src={obiWan}
            alt="Obi Wan Kenobi waving at you"
            class="w-1/2 max-w-xs pt-2"
        />
    </div>
</Layout>
